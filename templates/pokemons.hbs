<div class="row">
  <div class="col-md-8">
    <table class="table table-hover">
      <thead>
        <tr>
          <th>Name</th>
        </tr>
      </thead>
      <tbody>
        {{#each}}
          {{#link-to "pokemon" this tagName="tr"}}
            <td>{{name}}</td>
          {{/link-to}}
        {{/each}}
      </tbody>
    </table>
  </div>
  <div class="col-md-2">
    {{outlet}}
  </div>
</div>
